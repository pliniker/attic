all: Makefile  /usr/local/thebrain/lib/$(TARGET)

/usr/local/thebrain/lib/$(TARGET):  $(UICDECLS) $(OBJECTS) $(OBJMOC) $(SUBLIBS) $(OBJCOMP)  
	test -d /usr/local/thebrain/lib/ || mkdir -p /usr/local/thebrain/lib/
